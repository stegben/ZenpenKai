{
  "name": "zenpen-kai",
  "version": "0.1.0",
  "description": "simple UI for fiction writers",
  "main": "index.js",
  "scripts": {
    "test-only": "ava",
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "build": "babel test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stegben/ZenpenKai.git"
  },
  "author": "cph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stegben/ZenpenKai/issues"
  },
  "homepage": "https://github.com/stegben/ZenpenKai#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-yoctol": "^0.4.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "react": "^15.0.2"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "match": [
      "*oo",
      "!foo"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  }
}
