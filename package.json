{
  "name": "zenpen-kai",
  "version": "0.1.0",
  "description": "simple UI for fiction writers",
  "main": "index.js",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "test-only": "mocha --compilers js:babel-register",
    "lint": "eslint .",
    "coverage": "nyc --check-coverage --lines 90 --functions 95 npm run test-only",
    "test": "npm run lint && npm run coverage",
    "build:prod": "rm -rf public && NODE_ENV=production webpack --config webpack.config.prod.js",
    "start:dev": "npm test && NODE_ENV=development node run.js",
    "start": "npm test && npm run build:prod && NODE_ENV=production node run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stegben/ZenpenKai.git"
  },
  "author": "cph",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stegben/ZenpenKai/issues"
  },
  "homepage": "https://github.com/stegben/ZenpenKai#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-yoctol": "^0.4.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "nyc": "^6.4.3",
    "react": "^15.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-tap-event-plugin": "^1.0.0"
  }
}
